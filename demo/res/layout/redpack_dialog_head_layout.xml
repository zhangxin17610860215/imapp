<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/redpack_head_shape_bg"
    android:id="@+id/rl_redpackDialog_head_bg"
    android:layout_marginRight="-10dp">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:background="@drawable/redpack_head_shape_bg">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="3dp"
            android:background="@drawable/redpack_head_shape2_bg">
            <ImageView
                android:id="@+id/img_redpackDialog_head"
                android:layout_width="@dimen/dp_60"
                android:layout_height="@dimen/dp_60"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
